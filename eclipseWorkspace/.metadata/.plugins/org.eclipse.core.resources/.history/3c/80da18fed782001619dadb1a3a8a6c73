#include<iostream>
using namespace std;
int main(){
int n;
cin>>n;
int up[n];
int down[n];
int temp=1;
for(int counter=0;counter<n;){
	if(temp%2==1){
	for(int i=0;i<temp;i++){
		cout<<"ÆæÊýÁË¡£"<<"counter is"<<counter<<",temp is"<<temp<<endl;
	up[counter]=temp-i;
	down[counter]=i+1;
	counter++;}
	cout<<up[counter-1]<<"/"<<down[counter-1]<<endl;
	}else{
		for(int i=0;i<temp;i++){
			up[counter]=i+1;
			down[counter]=temp-i;
			counter++;
			cout<<up[counter-1]<<"/"<<down[counter-1]<<endl;
			}
	}
	temp++;
}
cout<<up[n-1]<<"/"<<down[n-1]<<endl;
	return 0;
}
