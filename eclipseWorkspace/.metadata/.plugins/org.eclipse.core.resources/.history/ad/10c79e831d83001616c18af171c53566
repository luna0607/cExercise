#include<iostream>
#include<math.h>
int jieCheng(int n);
double sin(int x);
int main(){
return 0;
}
double sin(int x){
double single;
double result;

for(int i=0;i<10000;i++){
single=pow(-1,i)*pow((double)x,(double)2i-1)/jieCheng(2*i-1);
}
}
int jieCheng(int n){
	int resulT=1;
	for(int i=1;i<=n;i++){
		resulT=resulT*i;
	}
	return resulT;
}
